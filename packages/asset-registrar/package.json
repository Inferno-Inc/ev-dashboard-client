{
  "name": "@ev-dashboard-client/asset-registrar",
  "version": "1.0.0",
  "description": "API for AssetOperators to enable their assets to participate in the ev-dashboard.",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "build:typechain": "typechain --target ethers-v4 --outDir ethers-types './test/contracts/*'",
    "start-rpc": "run-with-testrpc -m \"candy maple cake sugar pudding cream honey rich smooth crumble sweet treat\" --port 8544 --accounts 20 --networkId=9 --gasLimit=10000000",
    "test:watch": "npm run start-rpc -- \"jest --coverage --watchAll\"",
    "test:prod": "npm run start-rpc -- \"jest --no-cache\""
  },
  "author": "Energy Web",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@ethersproject/bytes": "^5.0.8",
    "@ethersproject/contracts": "^5.0.8",
    "@ethersproject/providers": "^5.0.16",
    "@ethersproject/wallet": "^5.0.8",
    "@ew-did-registry/keys": "0.0.1-alpha.694.1",
    "better-sqlite3": "^5.4.3",
    "ethers": "^4.0.48",
    "web3-utils": "^1.3.0"
  },
  "devDependencies": {
    "@typechain/ethers-v4": "~4.0.0",
    "@types/jest": "~26.0.21",
    "jest": "~26.6.3",
    "rimraf": "~3.0.2",
    "run-with-testrpc": "~0.3.1",
    "typescript": "~4.2.3",
    "typechain": "~4.0.3",
    "ts-jest": "~26.5.4"
  }
}
